/*reset vc*/


/**********/
.vc_section{
  padding:0;
  margin:0;
  display:flex;
  flex-wrap:wrap;
  align-items:center;
  flex-direction:column;
}
.vc_section > div{
  min-height:0%;
}
.vc_row {
  -js-display: flex;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  vertical-align:bottom;
  margin:0 -0.5rem;
}
.wpb_column.vc_column_container{
  /*padding:0 0.5rem;*/
}

.vc_column_container>.vc_column-inner,
.vc_column-inner {
  padding: 0;
}

.wpb_button,
.wpb_content_element,
ul.wpb_thumbnails-fluid>li {
  margin-bottom: 0px;
}

.vc_column-inner::after, .vc_column-inner::before,
.vc_row:after, .vc_row:before{
  content:none;
}












